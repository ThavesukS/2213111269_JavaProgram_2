import javax.swing.*;
public class Lab605 {

	static int[] number = new int[5];
	public static void main(String[] args) {
		
		
		for(int i=0; i<number.length;i++) {
		JOptionPane.showInputDialog("Input number "+(i+1)+" :");
		}
		
		ShowEven(number);

	}
	public static void ShowEven(int[] num) {
		String msg="";
		for(int i = 0 ; i<num.length;i++) {
			if(num[i]%2==0) {
				msg += num[i];
			}
			JOptionPane.showMessageDialog(null, "list of even number:\n"+number[i]);
		}
	}

}
